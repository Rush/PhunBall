
function logMsg(text) {
	$('#console').append("<span>" + text + "</span>\n");
}

function onConnected() {
	logMsg("Connected to server");
}

function onPlayerConnected(id, x, y)  {
	logMsg("Player " + id + " joined at x=" + x + " y=" + y);	
}

